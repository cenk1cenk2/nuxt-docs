{
  "name": "@cenk1cenk2/nuxt-content-theme-docs",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/",
  "types": "./src/index.d.ts",
  "description": "Fork of @nuxt/content-theme-docs for my own taste.",
  "keywords": [
    "nuxt",
    "nuxt-content",
    "vue"
  ],
  "license": "MIT",
  "repository": "https://github.com/cenk1cenk2/nuxt-docs",
  "author": {
    "name": "Cenk Kilic",
    "url": "https://cenk.kilic.dev",
    "email": "cenk@kilic.dev"
  },
  "scripts": {
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js --fix && yarn lint:style --fix",
    "lint:check": "eslint --ext .ts,.js,.tsx,.jsx,.vue ./",
    "lint:staged": "lint-staged",
    "docs:toc": "markdown-toc README.md --bullets='-' -i"
  },
  "lint-staged": {
    "./*.{ts,js,tsx,jsx,vue}": [
      "prettier --loglevel warn --write",
      "eslint --fix"
    ],
    "./*.{json,md}": [
      "prettier --loglevel warn --write"
    ],
    "*.{css,vue}": [
      "stylelint --fix"
    ]
  },
  "files": [
    "src"
  ],
  "dependencies": {
    "@docsearch/css": "^1.0.0-alpha.28",
    "@docsearch/js": "^1.0.0-alpha.28",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@nuxt/content": "^1.14.0",
    "@nuxt/types": "^2.15.8",
    "@nuxtjs/color-mode": "^2.1.1",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/google-fonts": "1.3.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/tailwindcss": "^4.0.3",
    "@tailwindcss/typography": "0.4.1",
    "autoprefixer": "^10.3.4",
    "clipboard": "^2.0.8",
    "consola": "^2.15.3",
    "defu": "^5.0.0",
    "execa": "^5.0.0",
    "highlight.js": "^11.2.0",
    "lodash.groupby": "^4.6.0",
    "marked": "^3.0.2",
    "@nuxtjs/i18n": "^7.0.3",
    "postcss": "^8.2.10",
    "prism-theme-one-dark": "^1.0.0",
    "prism-themes": "^1.6.0",
    "shiki": "^0.9.10",
    "shiki-twoslash": "^3.0.1",
    "tailwind-css-variables": "^3.0.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "theme-colors": "^0.0.5",
    "vue-scrollactive": "^0.9.3"
  },
  "devDependencies": {
    "@nuxtjs/composition-api": "^0.25.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.19.0",
    "nuxt": "^2.15.4",
    "tailwindcss-dark-mode": "^1.1.7"
  }
}
